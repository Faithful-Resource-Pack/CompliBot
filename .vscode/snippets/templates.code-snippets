	"New command template": {
		"prefix": "nct",
		"body": [
			"import { SlashCommand, Client } from '@interfaces';",
			"import { CommandInteraction } from 'discord.js';",
			"import { SlashCommandBuilder } from '@discord.js/builders';",
			"export const command: SlashCommand = {",
			"\tpermissions: {",
			"\t\troles: [], //roles array",
			"\t\tusers: [], //userID array",
			"\t},",
			"\tdata: new SlashCommandBuilder()",
			"\t\t.setDefaultPermission(true)",
			"\t\t.setName(\"${1:Name}\")",
			"\t\t.setDescription(`${2:Description}`),",
			"\texecute: async (interaction: CommandInteraction, client: Client) => {",
			"\t\t${3://todo: implement command}",
			"\t},\n};"
		]
	},
