{
    "name": "complibot",
    "version": "1.0.0",
    "description": "a discord bot for the compliance texture pack",
    "main": "src/index.ts",
    "type": "module",
    "_moduleAliases": {
        "~/*": [
            "dist/src"
        ],
        "@/*": [
            "dist/"
        ]
    },
    "scripts": {
        "start": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts"
    },
    "devDependencies": {
