{
    "name": "complibot",
    "version": "2.0.1",
    "description": "A discord bot for the compliance texture pack",
    "main": "src/index.ts",
    "type": "commonjs",
    "scripts": {
        "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
        "start": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
        "genLang": "node -r ts-node/register -r tsconfig-paths/register src/genLangReadme.ts",
        "prettier": "prettier src/**/*.ts --config .prettierrc --write"
    },
    "devDependencies": {
        "prettier": "^2.5.1",
        "ts-node": "^10.6.0",
        "ts-node-dev": "^1.1.8",
        "tsc-alias": "^1.6.3",
        "tsconfig-paths": "^3.13.0",
        "typescript": "^4.6.2"
    },
    "dependencies": {
        "@discordjs/rest": "^0.3.0",
        "@types/node": "^17.0.21",
        "axios": "^0.26.0",
        "canvas": "^2.9.0",
        "chalk": "^4.1.2",
        "discord-api-types": "^0.26.1",
        "discord-slash-commands-client": "^1.2.2",
        "discord.js": "^13.6.0",
        "firestorm-db": "^1.10.0",
        "follow-redirects": "^1.14.9",
        "image-size": "^1.0.1",
        "jszip": "^3.7.1",
        "linux-os-info": "^2.0.0",
        "module-alias": "^2.2.2",
        "moment": "^2.29.1",
        "tslib": "^2.3.1"
    }
}