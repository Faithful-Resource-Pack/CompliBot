        "typescript-eslint": "^8.37.0"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.74",
        "@octokit/rest": "^22.0.0",
        "axios": "^1.10.0",
        "chalk": "^5.4.1",
        "discord.js": "^14.21.0",
        "gif-encoder-2": "^1.0.5",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0"
