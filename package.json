{
    "name": "complibot",
    "version": "2.5.0",
    "description": "A discord bot for the Faithful Team Resource Packs",
    "main": "src/index.ts",
    "type": "commonjs",
    "scripts": {
        "eslint": "npx eslint . --ext .ts,.tsx --fix",
        "dev": "npm run eslint && ts-node-dev --stack-size=1024 --stack-trace-limit=1000 --respawn -r tsconfig-paths/register src/index.ts",
        "start": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
        "start-prod": "npm run build && cross-env NODE_ENV=production node --stack-size=1024 --stack-trace-limit=1000 dist/src/index.js --",
        "clean": "rimraf dist/",
        "copy-files": "copyfiles src/**/*.log dist/",
        "build": "npm run clean && tsc --project tsconfig.json && tsc-alias -p tsconfig.json -v && npm run copy-files"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.33",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.2.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.25.2",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.7.0",
        "ts-node-dev": "^1.1.8",
        "tsc-alias": "^1.6.9",
        "tsconfig-paths": "^4.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@discordjs/builders": "^0.13.0",
        "@discordjs/rest": "^0.4.1",
        "axios": "^0.26.1",
        "body-parser": "^1.20.0",
        "canvas": "^2.10.2",
        "chalk": "^4.1.2",
        "discord-api-types": "^0.30.0",
        "discord.js": "^13.8.0",
        "express": "^4.18.1",
        "firestorm-db": "^1.10.0",
        "follow-redirects": "^1.15.0",
        "gif-encoder-2": "^1.0.5",
        "image-size": "^1.0.1",
        "jszip": "^3.9.1",
        "linux-os-info": "^2.0.0",
        "module-alias": "^2.2.2",
        "moment": "^2.29.3"
    }
}
