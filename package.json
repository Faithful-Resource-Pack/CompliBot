{
    "name": "complibot",
    "version": "4.0.0",
    "description": "The official bot for the Faithful Discord servers.",
    "main": "src/index.ts",
    "scripts": {
        "start": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
