{
    "name": "complibot",
    "version": "2.0.4",
    "description": "A discord bot for the Faithful Team Resource Packs",
    "main": "src/index.ts",
    "type": "commonjs",
    "scripts": {
        "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
        "start": "node -r ts-node/register -r tsconfig-paths/register src/index.ts",
        "genLang": "node -r ts-node/register -r tsconfig-paths/register src/genLangReadme.ts",
        "prettier": "prettier src/**/*.ts --config .prettierrc --write"
    },
    "devDependencies": {
        "prettier": "^2.6.2",
        "ts-node": "^10.7.0",
        "ts-node-dev": "^1.1.8",
        "tsc-alias": "^1.6.6",
        "tsconfig-paths": "^3.14.1",
        "typescript": "^4.6.3"
    },
    "dependencies": {
        "@discordjs/rest": "^0.4.1",
        "@types/node": "^17.0.25",
        "axios": "^0.26.1",
        "canvas": "^2.9.1",
        "chalk": "^4.1.2",
        "discord-api-types": "^0.26.1",
        "discord-slash-commands-client": "^1.2.2",
        "discord.js": "^13.6.0",
        "firestorm-db": "^1.10.0",
        "follow-redirects": "^1.14.9",
        "image-size": "^1.0.1",
        "jszip": "^3.9.1",
        "linux-os-info": "^2.0.0",
        "module-alias": "^2.2.2",
        "moment": "^2.29.3",
        "tslib": "^2.3.1"
    }
}