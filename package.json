    },
    "dependencies": {
        "@discordjs/rest": "^0.2.0-canary.0",
        "@types/node": "^17.0.7",
        "axios": "^0.24.0",
        "canvas": "^2.8.0",
        "discord-api-types": "^0.26.1",
        "discord-slash-commands-client": "^1.2.2",
        "discord.js": "^13.5.0",
        "firestorm-db": "^1.8.0",
        "follow-redirects": "^1.14.6",
        "image-size": "^1.0.0",
        "moment": "^2.29.1",
        "slash-commands": "^1.5.0"
    }
